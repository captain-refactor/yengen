{
    "name": "yengen",
    "version": "0.1.1",
    "author": "janexpando",
    "bin": {
        "yengen": "./bin/run"
    },
    "bugs": "https://github.com/janexpando/yengen/issues",
    "dependencies": {
        "@oclif/command": "^1.5.13",
        "@oclif/config": "^1.13.0",
        "@oclif/parser": "^3.8.0",
        "@oclif/plugin-help": "^2.1.6",
        "@types/change-case": "^2.3.1",
        "@types/joi": "^14.3.3",
        "@types/koa-joi-router": "^5.2.2",
        "@types/lodash": "^4.14.129",
        "@types/rimraf": "^2.0.2",
        "change-case": "^3.1.0",
        "injection-js": "^2.2.1",
        "joi": "^14.3.1",
        "jsonref": "^5.1.3",
        "koa-joi-router": "^6.0.0",
        "lodash": "^4.17.11",
        "prettier": "^1.17.1",
        "rimraf": "^2.6.3",
        "swagger-parser": "^6.0.5",
        "ts-morph": "^2.1.0",
        "tslib": "^1.9.3",
        "typescript": "^3.4.5"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.22.0",
        "@oclif/test": "^1.2.4",
        "@oclif/tslint": "^3.1.1",
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.14.7",
        "chai": "^4.2.0",
        "globby": "^8.0.2",
        "mocha": "^5.2.0",
        "nyc": "^13.3.0",
        "ts-node": "^8.1.0",
        "tslint": "^5.16.0"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/janexpando/yengen",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "yengen",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "janexpando/yengen",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "test:watch": "mocha -w",
        "check": "tsc -p tsconfig.check.json",
        "version": "oclif-dev readme && git add README.md",
        "tslint:fix": "tslint --fix --project .",
        "editor": "docker run -d -p 80:8080 swaggerapi/swagger-editor"
    },
    "types": "lib/index.d.ts"
}
